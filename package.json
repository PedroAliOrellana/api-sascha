{
  "name": "api-sascha",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "start": "node ./app.js",
    "update:db": "psql saschadb < $PWD/db/saschadb.sql",
    "remove:db": "dropdb -e --if-exists -h localhost -p 5432 -U postgres saschadb",
    "create:db": "sudo -u postgres psql -c 'create database saschadb;'",
    "backup:db": "pg_dump saschadb -h localhost -p 5432 -U postgres -F p -b -v -f $PWD/db/saschadb.sql",
    "restore:db": "npm run remove:db && npm run create:db && npm run update:db"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "~1.17.1",
    "bookshelf": "^0.12.1",
    "compression": "^1.7.2",
    "consolidate": "^0.15.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "express-handlebars": "^3.0.0",
    "graceful-fs": "^4.1.11",
    "jwt-simple": "^0.5.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.5",
    "method-override": "^2.3.10",
    "moment": "^2.21.0",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.3",
    "pg": "^7.4.1",
    "serve-favicon": "~2.4.2",
    "swig": "^1.4.2",
    "when": "^3.7.8"
  }
}
